<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>java_servlet.login_severlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>reg</servlet-name>
  <servlet-class>java_servlet.reg_servlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>modify</servlet-name>
  <servlet-class>java_servlet.modify_servlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>delete</servlet-name>
  <servlet-class>java_servlet.delete_servlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>chun</servlet-name>
  <servlet-class>java_servlet.chun_servlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>xia</servlet-name>
  <servlet-class>java_servlet.xia_servlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>qiu</servlet-name>
  <servlet-class>java_servlet.qiu_servlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>dong</servlet-name>
  <servlet-class>java_servlet.dong_servlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>gu</servlet-name>
  <servlet-class>java_servlet.gu_servlet</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/login</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>reg</servlet-name>
  <url-pattern>/reg</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>modify</servlet-name>
  <url-pattern>/modify</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>delete</servlet-name>
  <url-pattern>/delete</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>chun</servlet-name>
  <url-pattern>/chun</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>xia</servlet-name>
  <url-pattern>/xia</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>qiu</servlet-name>
  <url-pattern>/qiu</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>dong</servlet-name>
  <url-pattern>/dong</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>gu</servlet-name>
  <url-pattern>/gu</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>index.jsp</welcome-file>
 </welcome-file-list>
 <login-config>
  <auth-method>BASIC</auth-method>
 </login-config>
</web-app>
